{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
        "url": "https://api.beta.ons.gov.uk/v1/data?uri=/peoplepopulationandcommunity/populationandmigration/populationestimates/timeseries/ukpop/pop",
        "format": {
            "property": "years"
        }
    },
    "title": "UK Population",
    "width": 200,
    "height": 200,
    "mark": "line",
    "encoding": {
        "x": {
            "field": "date",
            "type": "temporal"
        },
        "y": {
            "field": "value",
            "type": "quantitative"
        }
    },
    "config": {}
}